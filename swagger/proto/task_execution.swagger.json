        },
        "taskArgs": {
          "$ref": "#/definitions/ga4gh_task_execTaskArgs",
          "title": "arguments for task instance"
        },
        "taskId": {
          "type": "string",
          "format": "string",
          "title": "ID of the task that will be used to create this task instance"
